{
	"compilerOptions": {
		"target": "esnext",
		"module": "esnext",
		"moduleResolution": "node",
		"importHelpers": true,
		"jsx": "react-jsx",
		"esModuleInterop": true,
		"sourceMap": true,
		"baseUrl": "./",
		"strict": false,
		"paths": {
			"@/*": ["src/*"],
			"@@/*": ["src/.umi/*"]
		},
		"allowSyntheticDefaultImports": true
	},
	"include": [
		"mock/**/*",
		"src/**/*",
		"config/**/*",
		".umirc.ts",
		"typings.d.ts"
, "public/ipc.js"	]
}
