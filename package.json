{
	"name": "neuron",
	"version": "0.1.0",
	"private": true,
	"main": "public/electron.js",
	"homepage": "./",
	"build": {
		"appId": "com.electron.electron-with-create-react-app",
		"directories": {
			"buildResources": "public"
		}
	},
	"dependencies": {
		"@material-ui/core": "^4.11.1",
		"@material-ui/icons": "^4.9.1",
		"@testing-library/jest-dom": "^5.11.4",
		"@testing-library/react": "^11.1.0",
		"@testing-library/user-event": "^12.1.10",
		"@types/jest": "^26.0.15",
		"@types/node": "^12.0.0",
		"@types/react": "^16.9.53",
		"@types/react-dom": "^16.9.8",
		"@udecode/slate-plugins": "^0.71.2",
		"electron-is-dev": "^1.2.0",
		"fontsource-roboto": "^3.0.3",
		"immutable": "^4.0.0-rc.12",
		"lodash": "^4.17.20",
		"react": "^17.0.1",
		"react-dom": "^17.0.1",
		"react-grid-layout": "^1.2.0",
		"react-scripts": "4.0.1",
		"robotjs": "^0.6.0",
		"slate": "^0.59.0",
		"slate-history": "^0.59.0",
		"slate-hyperscript": "^0.59.0",
		"slate-react": "^0.59.0",
		"source-map-explorer": "^2.5.1",
		"styled-components": "^5.2.1",
		"typescript": "^4.0.3",
		"web-vitals": "^0.2.4"
	},
	"scripts": {
		"start": "concurrently \"npm run react-start\" \"wait-on http://localhost:3002 && electron .\"",
		"analyze": "source-map-explorer 'build/static/js/*.js'",
		"react-start": "react-app-rewired start",
		"react-build": "react-scripts build",
		"react-test": "react-scripts test",
		"react-eject": "react-scripts eject",
		"electron-build": "electron-builder",
		"release": "npm run react-build && electron-builder --publish=always",
		"build": "npm run react-build && npm run electron-build",
		"elev": "electron -v && electron -a",
		"rebuild": "npm rebuild --runtime=electron --target=11.0.3 --disturl=https://atom.io/download/atom-shell --abi=85"
	},
	"eslintConfig": {
		"extends": [
			"react-app",
			"react-app/jest"
		]
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"devDependencies": {
		"@types/react-grid-layout": "^1.1.1",
		"babel-plugin-import": "^1.13.3",
		"babel-plugin-styled-components": "^1.12.0",
		"concurrently": "^5.3.0",
		"customize-cra": "^1.0.0",
		"electron": "^11.0.3",
		"electron-builder": "^22.9.1",
		"react-app-rewired": "^2.1.6",
		"wait-on": "^5.2.0"
	}
}
